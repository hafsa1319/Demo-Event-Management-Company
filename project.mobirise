          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing1\" group=\"Pricing\" mbr-class=\"{'mbr-parallax-background': bg.parallax, 'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width Title\" min=\"4\" max=\"12\" value=\"9\" step=\"1\" condition=\"showTitle || showSubtitle\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Color\" name=\"borderColor\" value=\"#000000\" condition=\"showBorder\">\n        <header>Cards</header>\n        <input type=\"color\" name=\"iconColor\" title=\"Icon Color\" value=\"#ffcc1a\">\n        <input type=\"color\" title=\"Border\" value=\"#0b8167\" name=\"border\">\n        <input type=\"color\" title=\"BG\" value=\"#006b54\" name=\"cardBg\">\n        <header>Cards 1</header>\n        <input type=\"range\" inline title=\"Icons\" name=\"itemsOne\" min=\"1\" max=\"5\" step=\"1\" value=\"3\">\n        <header>Cards 2</header>\n        <input type=\"range\" inline title=\"Icons\" name=\"itemsTwo\" min=\"1\" max=\"5\" step=\"1\" value=\"4\">\n        <header>Cards 3</header>\n        <input type=\"range\" inline title=\"Icons\" name=\"itemsThree\" min=\"1\" max=\"5\" step=\"1\" value=\"5\">\n        <header>Bac